select
    a.name,
    t._id,
    t.timestamp,
    t.value_eth,
    l.params [3].value
from
    sort.transaction_log l,
    sort.transaction t,
    sort.address a
where
    LOWER(t.to) = '0x7f268357a8c2552623316e2562d90e642bb538e5'
    and l.transaction_hash = t._id
    and l.name = 'Approval'
    and l.params [3].name = 'tokenId'
    and a._id = l.function_address
order by
    timestamp desc
limit
    100
